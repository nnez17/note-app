{
  "name": "note-keep",
  "version": "1.0.0",
  "description": "A modern note-taking app with Firebase backend",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "clean": "rm -rf dist node_modules",
    "test": "bun test"
  },
  "keywords": [
    "notes",
    "firebase",
    "elysia",
    "bun",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "elysia": "^1.1.29",
    "@elysiajs/static": "^1.1.3",
    "@elysiajs/cors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
